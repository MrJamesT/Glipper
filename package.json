		"@electron-forge/maker-deb": "^6.0.5",
		"@electron-forge/maker-rpm": "^6.0.5",
		"@electron-forge/maker-squirrel": "^6.0.5",
		"@electron-forge/maker-zip": "^6.0.5",
		"@mdi/font": "^7.1.96",
		"@vitejs/plugin-vue": "^4.0.0",
		"concurrently": "^7.6.0",
		"cross-env": "^7.0.3",
		"electron": "23.1.3",
		"typescript": "^4.9.3",
		"vite": "^4.1.0",
		"vue-tsc": "^1.0.24"
